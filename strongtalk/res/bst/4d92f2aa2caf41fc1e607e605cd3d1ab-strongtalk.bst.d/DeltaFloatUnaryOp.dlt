! (Delta mirrorFor: #DeltaFloatUnaryOp) methodsFor: 'Unclassified' !


apply: aTool <DeltaTool > ^ <Self>
    aTool floatUnaryOpNode: self!

f
	^ f!

f: op x: arg
	self assert: [ EnableFloatValues ].
	self assert: [ arg isFloatExpressionNode ].
	f := op.
	x := arg!

isFloatUnaryOpNode
	^ true!

x
	^ x! !

